{
  "name": "biblioteca-jogos-typescript",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^2.0.0",
    "dotenv": "^16.0.3",
    "express": "^4.17.1",
    "joi": "^17.8.3",
    "jsonwebtoken": "^9.0.0",
    "knex": "0.21.17",
    "lodash": "4.17.21",
    "mysql2": "^3.1.2",
    "objection": "2.2.16",
    "prettier": "^2.8.4",
    "reflect-metadata": "^0.1.13"
  },
  "scripts": {
    "app:build:debug": "babel ./src --extensions \".js,.ts,.tsx\" --out-dir ./build/src --copy-files --no-copy-ignored --source-maps",
    "start": "nodemon -w src -e 'js,jsx,ts,tsx,json,hbs' --exec babel-node --extensions '.js,.jsx,.ts,.tsx' src/index.ts",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "migrate:latest": "babel-node ./node_modules/.bin/knex migrate:latest --knexfile ./database/knexfile.ts",
    "migrate:make": "knex migrate:make --knexfile ./database/knexfile.ts -x ts",
    "migrate:rollback": "babel-node ./node_modules/.bin/knex --knexfile ./database/knexfile.ts migrate:rollback"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.8",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/lodash": "4.14.171",
    "@types/node": "^18.13.0",
    "@types/validator": "^13.7.12",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^7.13.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "nodemon": "^2.0.20",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.0.5"
  }
}